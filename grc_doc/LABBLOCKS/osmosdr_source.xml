    <hide>#if $nchan() > 4 then 'none' else 'all'#</hide>
  </param>
  <param>
    <name>Ch4: IF Gain (dB)</name>
    <key>if_gain4</key>
    <value>20</value>
    <type>real</type>
    <hide>#if $nchan() > 4 then 'none' else 'all'#</hide>
  </param>
  <param>
    <name>Ch4: BB Gain (dB)</name>
    <key>bb_gain4</key>
    <value>20</value>
    <type>real</type>
